{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "githubActionDist",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectAcl",
        "s3:ListAllMyBuckets",
        "s3:GetObjectVersion",
        "s3:PutObject",
        "s3:PutObjectVersion",
        "s3:DeleteObject",
        "s3:DeleteObjectVersion",
        "s3:ObjectOwnerOverrideToBucketOwner",
        "s3:PutObjectAcl",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "${s3_arn}",
        "${other_arn}"
      ]
    }
  ]
}
